{
  "name": "georgewwalker.com",
  "description": "Personal Portfolio Website",
  "scripts": {
    "start": "react-scripts start",
    "build:quick": "react-scripts build && npm run postbuild",
    "prebuild": "npm i",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "deploy": "npm run build && gh-pages-deploy",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sync-master": "git pull git@github.com:pkmnct/georgewwalker.com.git"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pkmnct/staging.georgewwalker.com.git"
  },
  "license": "MIT",
  "gh-pages-deploy": {
    "staticpath": "build",
    "cname": "staging.georgewwalker.com",
    "prep": [
      "build"
    ],
    "noprompt": false
  },
  "dependencies": {
    "@octokit/rest": "^16.28.9",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/styled-components": "^4.1.18",
    "classnames": "^2.2.6",
    "ken-burns-slideshow": "^1.1.3",
    "particles.js": "^2.0.0",
    "react": "^16.9.0",
    "react-awesome-slider": "^1.0.1",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-snap": "^1.23.0",
    "styled-components": "^4.3.2",
    "typescript": "^3.6.2"
  },
  "devDependencies": {
    "gh-pages-deploy": "^0.5.1"
  }
}
